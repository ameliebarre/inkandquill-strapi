{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "text": {
      "type": "blocks"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "button": {
      "displayName": "button",
      "type": "component",
      "repeatable": false,
      "component": "category.button"
    },
    "slug": {
      "type": "string"
    },
    "books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book.book",
      "mappedBy": "category"
    }
  }
}
